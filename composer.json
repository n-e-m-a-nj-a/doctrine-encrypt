{
  "name": "n-e-m-a-nj-a/doctrine-encrypt",
  "description": "Doctrine ORM extension for searchable, owner-scoped encryption (symmetric & hybrid), with Symfony bundle, tests, CI and key-versioning support.",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "doctrine/orm": "^2.9 || ^2.10 || ^3.0",
    "doctrine/annotations": "^1.13 || ^2.0",
    "symfony/console": "^5.4 || ^6.0",
    "symfony/dependency-injection": "^5.4 || ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "doctrine/cache": "^1.11 || ^2.1",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.92"
  },
  "autoload": {
    "psr-4": {
      "NC\\DoctrineEncrypt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NC\\DoctrineEncrypt\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
